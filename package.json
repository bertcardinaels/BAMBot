{
  "name": "bam-bot",
  "version": "1.1.0",
  "description": "discord.js bot for use on the BAM Jonge Discord server",
  "main": "src/main.ts",
  "scripts": {
    "dev:remote": "cross-env NODE_ENV=remote ts-node-dev --respawn --transpile-only --poll ./src/main.ts",
    "dev:local": "cross-env NODE_ENV=local ts-node-dev --respawn --transpile-only --poll ./src/main.ts",
    "start": "cross-env NODE_ENV=local ts-node ./src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bertcardinaels/bam-bot.git"
  },
  "keywords": [
    "discord.js",
    "bot"
  ],
  "author": "Bert Cardinaels",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bertcardinaels/bam-bot/issues"
  },
  "homepage": "https://github.com/bertcardinaels/bam-bot#readme",
  "devDependencies": {
    "@types/node": "^16.6.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@discordjs/rest": "^0.1.0-canary.0",
    "@types/dotenv-flow": "^3.2.0",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.3.1",
    "dotenv-flow": "^3.2.0",
    "got": "^11.8.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
